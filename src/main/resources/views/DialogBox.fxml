<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root
   type="javafx.scene.layout.HBox"
   xmlns="http://javafx.com/javafx"
   xmlns:fx="http://javafx.com/fxml"
>
    <padding>
        <Insets top="8.0" left="8.0" right="8.0" />
    </padding>
    <ImageView fx:id="displayPicture" fitHeight="16.0" fitWidth="16.0" preserveRatio="true">
        <HBox.margin>
            <Insets right="6.0" />
        </HBox.margin>
    </ImageView>
    <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity" />
</fx:root>
