<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" minHeight="-Infinity" prefWidth="500.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" maxWidth="398.0" style="-fx-background-color: #eca65f; -fx-border-color: #85361a; -fx-border-radius: 3; -fx-border-width: 3; -fx-background-radius: 3;" text="label" textFill="#56160c" wrapText="true" HBox.hgrow="NEVER">
             <font>
                <Font name="rainyhearts" size="18.0" />
             </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
             <HBox.margin>
                <Insets left="10.0" right="10.0" />
             </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
