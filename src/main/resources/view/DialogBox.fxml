<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" minHeight="10" minWidth="50"
                style="-fx-border-width:1; -fx-background-color:rgb(100, 200, 218); -fx-padding:10, 10, 10, 10; -fx-border-radius: 8; -fx-background-radius: 15; -fx-border-width:8px; -fx-border-color:rgb(244, 244, 244);"/>
        <ImageView fx:id="displayPicture" fitHeight="50" fitWidth="50" pickOnBounds="true" preserveRatio="true"
                style="-fx-padding:10, 0, 0, 10; -fx-height:10"/>
    </children>
    <padding>
        <Insets bottom="5.0" left="10.0" right="10.0" top="15.0" />
    </padding>
</fx:root>


