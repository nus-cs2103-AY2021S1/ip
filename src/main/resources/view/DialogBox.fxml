    <children>
        <Label fx:id="dialog" text="Label" wrapText="true">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin></Label>
      <Circle fx:id="imageCircle" fill="DODGERBLUE" radius="31.0" stroke="BLACK" strokeType="INSIDE" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</fx:root>
