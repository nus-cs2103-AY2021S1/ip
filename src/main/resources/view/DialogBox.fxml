            <Font name="Comic Sans MS" size="13.0" />
         </font>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="80.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
