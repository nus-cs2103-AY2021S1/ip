                        <Insets top="10.0" bottom="10.0" left="10.0" right="10.0" />
                    </padding>
                </Label>
            </children>
        </HBox>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</fx:root>
