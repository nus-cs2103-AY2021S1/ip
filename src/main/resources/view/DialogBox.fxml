         xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minHeight="-Infinity"
               text="Label" wrapText="true"
               style="-fx-background-color: rgb(220,220,220); -fx-background-radius: 20; -fx-border-radius: 20;">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" >
            <clip>
                <Circle centerX="49.5" centerY="49.5" radius="49.5" />
            </clip>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
