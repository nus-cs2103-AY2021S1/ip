<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity" prefWidth="400.0"
         spacing="20" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/10.0.2"
         xmlns:fx="http://javafx.com/fxml/">
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
    <Label fx:id="dialog" text="Label" wrapText="false"/>
    <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true"/>
</fx:root>


