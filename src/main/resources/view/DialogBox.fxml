<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin></Label>
      <Circle fx:id="displayPicture" fill="#1f93ff00" radius="50.0" stroke="TRANSPARENT" strokeType="INSIDE">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Circle>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
