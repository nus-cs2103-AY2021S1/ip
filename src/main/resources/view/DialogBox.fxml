    <children>
        <Label fx:id="dialog" graphicTextGap="10.0" style="-fx-background-color: #08364E; -fx-padding: 7px; -fx-border-radius: 3px; -fx-background-radius: 5px; -fx-background-insets: 0,1; -fx-effect: innershadow(three-pass-box, rgba(0,0,0,0.75),1,0,1,1);" text="Label" textFill="#e1e0e0" wrapText="true" />
