<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="192.0" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" prefHeight="52.0" prefWidth="290.0" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
         <HBox.margin>
            <Insets right="10.0" top="10.0" />
         </HBox.margin></Label>
      <Circle fx:id="displayPicture" fill="DODGERBLUE" radius="35.0" stroke="BLACK" strokeType="INSIDE" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
