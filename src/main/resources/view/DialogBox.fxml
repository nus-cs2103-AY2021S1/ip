    <children>
        <Label fx:id="dialog" prefWidth="273.0" style="-fx-border-color: black; -fx-background-color: lavender; -fx-border-radius: 5; -fx-padding: 10;" textFill="#656565" HBox.hgrow="ALWAYS" wrapText="true">
