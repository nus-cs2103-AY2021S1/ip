<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<fx:root alignment="TOP_RIGHT"  maxHeight="-Infinity" minHeight="-Infinity" maxWidth="1.7976931348623157E308" prefWidth="400.0"
         style="-fx-background-color: #494B61;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx" 
         xmlns:fx="http://javafx.com/fxml">
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0"/>
    </padding>
    <Label fx:id="dialog" text="Label" textFill="WHITE" wrapText="true">
        <HBox.margin>
        <Insets top="15.0"/>
        </HBox.margin>
    </Label>
    <ImageView fx:id="displayPicture" disable="true" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true"
               preserveRatio="true">
        <HBox.margin>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
        </HBox.margin>
    </ImageView>
</fx:root>
