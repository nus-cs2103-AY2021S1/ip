<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<?import javafx.scene.text.Font?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" stylesheets="@Styles.css">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity" styleClass="messageWindow" graphicTextGap="5" textFill="chartreuse">
        <font>
            <Font name="Trebuchet MS Bold" size="10.0"/>
        </font></Label>
      <Circle fx:id="displayPicture" accessibleRole="IMAGE_VIEW" fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
