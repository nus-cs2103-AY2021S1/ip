<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity" >
            <effect>
                <InnerShadow />
            </effect>
            <font>
                <Font name="Consolas" size="12.0" />
            </font>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="105.0" fitWidth="105.0" pickOnBounds="true" preserveRatio="true">
        </ImageView>
    </children>
    <padding>
        <Insets bottom="3.0" left="15.0" right="15.0" top="3.0" />
    </padding>
   <effect>
      <InnerShadow />
   </effect>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</fx:root>
