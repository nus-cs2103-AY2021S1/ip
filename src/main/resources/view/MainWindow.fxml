<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="598.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainWindow">
    <children>
        <TextField fx:id="userInput" layoutX="-2.0" layoutY="555.0" onAction="#handleUserInput" prefHeight="42.0" prefWidth="575.0" promptText="Type here..." AnchorPane.bottomAnchor="0.0">
         <font>
            <Font name="Gill Sans MT" size="16.0" />
         </font></TextField>
        <Button fx:id="sendButton" layoutX="573.0" layoutY="556.0" mnemonicParsing="false" onAction="#handleUserInput" prefHeight="41.0" prefWidth="76.0" text="SEND">
         <font>
            <Font size="18.0" />
         </font></Button>
        <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" hvalue="1.0" layoutY="-6.0" prefHeight="564.0" prefWidth="649.0" vvalue="1.0">
            <content>
                <VBox fx:id="dialogContainer" />
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
